class BaseComponent extends HTMLElement {

    gameCode = null;

    componentStyle = `
    <style>
        /* */
    </style>`;

    constructor() {
      super();
    }
  
    connectedCallback() {
        this.loadData();
        this.innerHTML = this.componentStyle + this.buildHTML();
    }

    loadData() {
        this.gameCode = Utils.getGameFromUrl();
    }

    buildHTML() {
        return ``;
    }
}

customElements.define('app-base', BaseComponent);
